<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Description>Push metrics to a PushGateaway for the Prometheus.Client</Description>
    <VersionPrefix>2.1.0</VersionPrefix>
    <TargetFrameworks>netstandard2.0;netstandard2.1</TargetFrameworks>
    <PackageId>Prometheus.Client.MetricPusher</PackageId>
    <RepositoryUrl>https://github.com/prom-client-net/prom-client-metricpusher</RepositoryUrl>
    <AssemblyOriginatorKeyFile>../Prometheus.Client.MetricPusher.snk</AssemblyOriginatorKeyFile>
  </PropertyGroup>
  <ItemGroup>
    <None Include="../icon.png">
      <Pack>true</Pack>
      <Visible>false</Visible>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Prometheus.Client" Version="[4.5.2,5.0.0)" />
  </ItemGroup>
</Project>
